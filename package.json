  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "presets": [
        "es2015",
        "react"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": "test",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
